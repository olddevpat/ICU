    <section class="text-center download-section content-section bg-dark" id="register" style="background-image:url('/img/register.jpg');">
        <div class="container">
            <div class="col-lg-8 mx-auto">
                <div class="row">
                    <div class="col-lg-8 mx-auto">
                        <h2 class="text-dark bg-primary p-2 ">REGISTER HERE</h2>
                        <section class="position-relative py-4 py-xl-5">
                            <div class="container position-relative">
                                <div class="row d-flex justify-content-center">
                                    <div class="col-12 col-sm-12 col-md-12 col-lg-12 col-xl-12 col-xxl-12">
                                        <div class="card mb-5">
                                            <div class="card-body p-3" id="register_form" >
                                                <form data-bss-subject="International chess university query" data-bss-recipient="cea4a954b885f4b1b7858c077a17cbbe" data-bss-loading-message="Creating your profile..." data-bss-success-title="Success" data-bss-success-message="Congrats You are now part of the University!">
                                                    <h3 class="text-dark">Become a Member now!</h3>
                                                    <div class="mb-3"><input class="form-control" type="text" id="name-2" name="name" placeholder="Name"></div>
                                                    <div class="mb-3"><input class="form-control" type="email" id="email" name="email" placeholder="Email"></div>
                                                    <div class="mb-3"><input class="form-control" type="number" id="age" placeholder="Age"></div>
                                                    <div class="mb-3">
                                                        <div class="form-check form-check-inline"><input class="form-check-input" type="radio" id="formCheck-1" name="category"><label class="form-check-label" for="formCheck-1" style="color: rgb(0,0,0);">STUDENT</label></div>
                                                        <div class="form-check form-check-inline"><input class="form-check-input" type="radio" id="formCheck-2" name="category"><label class="form-check-label" for="formCheck-2" style="color: rgb(0,0,0);">INSTRUCTOR</label></div>
                                                    </div>
                                                    <div><button class="btn btn-primary d-block w-100" type="submit">Sign me up</button></div>
                                                </form>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </section>
                        <ul class="list-inline banner-social-buttons overlay p-2">
                            <li class="list-inline-item">&nbsp;<button class="btn btn-primary btn-lg btn-default" type="button"><i class="fa fa-facebook fa-fw"></i><span class="network-name">&nbsp; facebook</span></button></li>
                            <li class="list-inline-item">&nbsp;<button class="btn btn-primary btn-lg btn-default" type="button"><i class="fa fa-linkedin fa-fw"></i><span class="network-name">&nbsp;linkedin</span></button></li>
                            <li class="list-inline-item">&nbsp;<button class="btn btn-primary btn-lg btn-default" type="button"><i class="fa fa-instagram fa-fw"></i><span class="network-name">&nbsp;instagram</span></button></li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <section class="text-center content-section" id="contact">
        <div class="container">
            <div class="row">
                <div class="col-lg-8 mx-auto">
                    <h2>We want to hear from you!</h2>
                    <section class="position-relative py-3 py-xl-5">
                        <div class="container position-relative">
                            <div class="row d-flex justify-content-center">
                                <div class="col-12 col-sm-12 col-md-12 col-lg-12 col-xl-12 col-xxl-12">
                                    <div class="card mb-5">
                                        <div class="card-body p-sm-5">
                                            <form data-bss-subject="International chess university query" data-bss-recipient="cea4a954b885f4b1b7858c077a17cbbe" data-bss-loading-message="Sending your message" data-bss-success-title="Success" data-bss-success-message="Your message has been delivered">
                                                <div class="mb-3"><input class="form-control" type="text" id="name-1" name="name" placeholder="Name"></div>
                                                <div class="mb-3"><input class="form-control" type="email" id="email-1" name="email" placeholder="Email"></div>
                                                <div class="mb-3"><textarea class="form-control" id="message-1" name="message" rows="6" placeholder="Message"></textarea></div>
                                                <div><button class="btn btn-primary d-block w-100" type="submit">Send</button></div>
                                            </form>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </section>
